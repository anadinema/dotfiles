# Template for AWS work profile

[sso-session pn-main]
sso_start_url = ${AWS_SSO_URL}
sso_region = eu-west-1
sso_registration_scopes = sso:account:access

[profile main]
sso_session = pn-main
sso_account_id = ${AWS_MAIN_ACCOUNT_ID}
sso_role_name = ${AWS_ACTIVE_ROLE}
region = eu-west-1

[profile dev]
sso_session = pn-main
sso_account_id = ${AWS_DEV_ACCOUNT_ID}
